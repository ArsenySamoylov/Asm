      <memberdef kind="define" id="_array_template_8h_1a0a4fd9b4b7f09d1e6de15f6597c6dcc2" prot="public" static="no">
        <name>ARR_CTOR</name>
        <param><defname>struct_name</defname></param>
        <param><defname>base_struct</defname></param>
        <initializer>int struct_name##Ctor (struct_name* arr)                                      \
    {                                                                         \
    assert(arr);                                                              \
                                                                              \
    arr-&gt;arr = (base_struct**) calloc (START_ARR_SIZE, sizeof(base_struct**));\
    assert(arr-&gt;arr);                                                         \
                                                                              \
    arr-&gt;size     = 0;                                                        \
    arr-&gt;capacity = START_ARR_SIZE;                                           \
                                                                              \
    return SUCCESS;                                                           \
    }</initializer>

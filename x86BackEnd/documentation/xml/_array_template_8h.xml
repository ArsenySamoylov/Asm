      <memberdef kind="define" id="_array_template_8h_1aecf1e1df694982cd3521dbb3dd847812" prot="public" static="no">
        <name>FIND_IN_ARR</name>
        <param><defname>struct_name</defname></param>
        <param><defname>find_struct</defname></param>
        <param><defname>cmp_function</defname></param>
        <initializer>find_struct* Find##find_struct (struct_name* arr, find_struct* find)     \
    {                                                                    \
    assert(arr);                                                         \
    assert(find);                                                        \
                                                                         \
    for (size_t i = 0; i &lt; arr-&gt;size; i++)                               \
        {                                                                \
        if (cmp_function(find, arr-&gt;arr[i]))                             \
            return arr-&gt;arr + i;                                         \
        }                                                                \
                                                                         \
    return NULL;                                                         \
    }</initializer>

      <memberdef kind="define" id="_array_template_8h_1a82eb4821a8884223e5ef374cfa8dac57" prot="public" static="no">
        <name>ARR_COPY</name>
        <param><defname>struct_name</defname></param>
        <param><defname>add_struct</defname></param>
        <initializer>int Copy##add_struct (struct_name *arr, const add_struct *val)  \
    {                                                       \
    assert(arr);                                            \
    assert(val);                                            \
                                                            \
    if (arr-&gt;size &gt;= arr-&gt;capacity)                         \
        struct_name##Resize (arr);                          \
                                                            \
    add_struct* temp = (add_struct*) calloc (1, sizeof (add_struct));  \
    assert     (temp);                                                 \
                                                            \
    memcpy (temp, val, sizeof (add_struct));                \
                                                            \
    arr-&gt;arr[arr-&gt;size++] = temp;                           \
    return 0;                                               \
    }</initializer>

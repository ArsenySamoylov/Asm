      <memberdef kind="define" id="_array_template_8h_1a480169a482d4ec6655bdd7d33b829067" prot="public" static="no">
        <name>ARR_RESIZE</name>
        <param><defname>struct_name</defname></param>
        <param><defname>base_struct</defname></param>
        <initializer>static int struct_name##Resize (struct_name* arr)                                          \
    {                                                                                       \
    assert(arr);                                                                            \
                                                                                            \
    size_t new_size = arr-&gt;capacity * GROWTH_RATE;                                          \
                                                                                            \
    base_struct** temp = (base_struct**) realloc (arr-&gt;arr, new_size * sizeof(temp[0]));    \
    assert(temp);                                                                           \
                                                                                            \
    arr-&gt;capacity = new_size;                                                               \
    arr-&gt;arr      = temp;                                                                   \
                                                                                            \
    return SUCCESS;                                                                         \
    }</initializer>

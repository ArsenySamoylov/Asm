      <memberdef kind="define" id="_array_template_8h_1a729738413382fe751c50caa7a8010176" prot="public" static="no">
        <name>ARR_DTOR</name>
        <param><defname>struct_name</defname></param>
        <param><defname>base_struct</defname></param>
        <initializer>int struct_name##Dtor (struct_name* arr)                        \
    {                                                           \
    assert(arr);                                                \
                                                                \
    if (arr-&gt;capacity == 0)                                     \
        {                                                       \
        report (&quot;Capacity can&apos;t be 0 in Dtor\n&quot;);               \
        return FAILURE;                                         \
        }                                                       \
                                                                \
    for (size_t i = 0; i &lt; arr-&gt;size; i++)                      \
        free(arr-&gt;arr[i]);                                      \
                                                                \
    free (arr-&gt;arr);                                            \
                                                                \
    arr-&gt;capacity = 0;                                          \
    arr-&gt;size     = 0;                                          \
    arr-&gt;arr      = NULL;                                       \
                                                                \
    return SUCCESS;                                             \
    }</initializer>
